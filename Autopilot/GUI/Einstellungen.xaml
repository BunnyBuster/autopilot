<Page x:Class="Autopilot.GUI.Einstellungen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="Einstellungen">
    <DockPanel>
        <ScrollViewer DockPanel.Dock="Right" HorizontalAlignment="Right">
        <StackPanel DockPanel.Dock="Top" Orientation="Vertical" HorizontalAlignment="Right" Margin="2">
            <Button Content="Flugzeugtypen" Click="Flugzeugtypen_anzeigen" Margin="2"/>
            <Button Content="noch was" Margin="2"/>
            <Button Content="und noch was" Margin="2"/>
        </StackPanel>
        </ScrollViewer>
        <Frame Name="Stammdatenanzeige" DockPanel.Dock="Left" HorizontalContentAlignment="Stretch" NavigationUIVisibility="Hidden" />
    </DockPanel>
</Page>
